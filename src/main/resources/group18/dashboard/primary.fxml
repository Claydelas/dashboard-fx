<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXSlider?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <MenuBar prefHeight="25.0" prefWidth="914.0" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Import Campaign" />
                  <MenuItem mnemonicParsing="false" text="Save as PNG" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Accessibility">
            <items>
              <MenuItem mnemonicParsing="false" text="UI Scaling" />
                  <MenuItem mnemonicParsing="false" text="Colour Palette" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <BorderPane BorderPane.alignment="CENTER">
         <center>
            <LineChart prefHeight="507.0" prefWidth="685.0">
              <xAxis>
                <CategoryAxis side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis side="LEFT" />
              </yAxis>
            </LineChart>
         </center>
         <bottom>
            <JFXSlider showTickLabels="true" showTickMarks="true" BorderPane.alignment="CENTER">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </JFXSlider>
         </bottom>
      </BorderPane>
   </center>
   <left>
      <VBox prefHeight="507.0" prefWidth="193.0" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
